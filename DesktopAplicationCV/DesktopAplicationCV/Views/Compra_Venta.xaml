<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DesktopAplicationCV.Views.Compra_Venta"
             xmlns:local="clr-namespace:DesktopAplicationCV.Views"
             Title="Compra y Venta">
    <TabBar>
        <Tab Title="Factura de Venta">
            <ShellContent
                          ContentTemplate="{DataTemplate local:Factura_Venta}" />
        </Tab>
        <Tab Title="Factura Venta Detalle">
            <ShellContent
                  ContentTemplate="{DataTemplate local:Factura_Venta_Detalle}" />
        </Tab>
        <Tab Title="Factura de Compra">
            <ShellContent
                  ContentTemplate="{DataTemplate local:Factura_Compra}" />
        </Tab>
        <Tab Title="Factura Compra Detalle">
            <ShellContent
          ContentTemplate="{DataTemplate local:Factura_Compra_detalle}" />
        </Tab>
    </TabBar>
</Shell>